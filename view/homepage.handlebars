<!DOCTYPE html>
<html>
<head>
	<title>Handlebars Example</title>
	<script src="https://cdn.jsdelivr.net/npm/handlebars@4.7.7/dist/handlebars.min.js"></script>
</head>
<body>
	<h1>Handlebars Example</h1>
	<div id="content"></div>

	<script id="template" type="text/x-handlebars-template">
		<ul>
			{{#each items}}
				<li>{{this}}</li>
			{{/each}}
		</ul>
	</script>

	<script>
		// Define the data for the template
		var data = {
			items: ['apple', 'banana', 'orange']
		};

		// Get the HTML source of the template
		var source = document.getElementById("template").innerHTML;

		// Compile the template source into a function
		var template = Handlebars.compile(source);

		// Render the template with the data and insert it into the page
		document.getElementById("content").innerHTML = template(data);
	</script>
</body>
</html>